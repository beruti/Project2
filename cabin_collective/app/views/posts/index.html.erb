<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>
		

<% @posts.each do |post| %>
	<br>
	<br>
	<br>
	<img src="<%= post.image %>">
	<%= post.name %>
	<br>
	<br>
	<br>
	<%= post.likes.count %>
	<br>

	<%=link_to has_liked?(post),"/posts/#{post.id}/like" %>
	<%= post.description %>	
<%end%>

