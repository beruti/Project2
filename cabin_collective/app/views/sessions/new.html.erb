<h6> views/sessions/new.html.erb</h6>

<h1>Welcome to Cabin Collective. <br>
Please Login Or Create a New User </h1>
<%= form_tag sessions_path do %>
 <div class="field">
 	<%= label_tag :email %>
 	<%= text_field_tag :email %>
 </div>
 <div class="field">
 <%= label_tag :password%>
 <%= password_field_tag :password%>
 </div>	

 <div class="actions"><%= submit_tag "Log in" %></div>
 <% end %>
